		jQuery('body').on('click', ".modal-buy__close", function() {
			jQuery.modal.close()
		})


		/*ABA CARACTERISTICAS*/
		jQuery("#paiCaracteristicas").click(function () {
		    jQuery(".Modo-de-usar, #video .Video, .Perguntas-Frequentes, .Caracteristicas-Tecnicas").hide();
			jQuery(".Sobre-o-produto").show();
		});

		/*ABA MODO DE USAR*/
		jQuery("#paiModoDeUsar").click(function () {
		    jQuery(".Sobre-o-produto, #video .Video, .Perguntas-Frequentes, .Caracteristicas-Tecnicas").hide();
			jQuery(".Modo-de-usar").show()
		});

		/*ABA VIDEO*/
		jQuery("#paiVideo").click(function () {
			jQuery(".Sobre-o-produto, .Modo-de-usar, .Perguntas-Frequentes, .Caracteristicas-Tecnicas").hide();
			jQuery("#video .Video").show();
		});

		/*ABA PERGUNTAS FREQUENTES*/
		jQuery("#paiPerguntasFrequentes").click(function () {
		    jQuery(".Sobre-o-produto, .Modo-de-usar, #video .Video, .Caracteristicas-Tecnicas").hide();
			jQuery(".Perguntas-Frequentes").show();
		});

		/*ABA CARACTERISTICAS TECNICAS*/
		jQuery("#paiCaracteristicasTecnicas").click(function () {
		    jQuery(".Sobre-o-produto, .Modo-de-usar, #video .Video, .Perguntas-Frequentes").hide();
			jQuery(".Caracteristicas-Tecnicas").show();
		});


		if( !jQuery('body').hasClass('kitlook') ){
			jQuery(".buy-in-page-button").on("click", function(a){
				a.preventDefault(),

				window.alert = function(){},

				alert = function(){

					vtexjs.checkout.getOrderForm()
					.then(function(orderForm){

						for(var k in orderForm.items){

							if(vtxctx.skus ==  orderForm.items[k].id){

								var item = orderForm.items[k];
									item.index = k;
							    	item.quantity = Number(jQuery('#tx_qtd_cr').val());

							    return vtexjs.checkout.updateItems([item]);

							}
						}
					})
					.done(function(orderForm){
						carrinhoUpdate();

						jQuery("#modal-buy").modal();
					});

				}
			})
		}


		/*
