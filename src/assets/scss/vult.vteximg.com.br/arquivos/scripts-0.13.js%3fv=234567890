        var cl = false;

         jQuery('.navbar-toggle').click(function(){
            if(cl){
                if( jQuery(".navbar-collapse").hasClass('in')){
                     jQuery(".navbar-collapse").removeClass('in');
                } else {
                     jQuery(".navbar-collapse").addClass('in').delay(1000).css('height', 'auto');
                }
            } else {
                cl = true;
            }
        });

        jQuery(document).click(function (e) {
            if (
                jQuery(e.target).parents(".navbar-collapse").length === 0 &&
                jQuery(e.target).parents(".navbar-header").length === 0 &&
                !jQuery(e.target).hasClass('navbar-toggle')
            ){
                jQuery(".navbar-collapse").removeClass('in');
            }
        });


		/* UPDATE NO CARRINHO */
		jQuery(window).on('cartProductAdded.vtex', function(evt, orderForm){
			carrinhoUpdate();
		});

		jQuery('.quantity-up, .quantity-down').on('click', function(event) {

			var total, t = jQuery('#tx_qtd_cr');

			if( jQuery(this).hasClass('quantity-up') ){
				total = parseInt(t.val()) + 1;
			} else {
				total = parseInt(t.val()) - 1;
				if (total == 0) {
					total = 1;
				}
			}

			t.val(total);

			jQuery('.buy-in-page-quantity').val(total).trigger('change');

			jQuery('.buy-in-page-button').prop('href', function(){
				return this.href.replace(/qty=\d([0-9]{0,})/, "qty=" + total)
			});


			return false;
		});

		jQuery("body").on('click', '.header-carrinho', function(e){

			if(jQuery(window).width() < 768) {
				e.preventDefault();
			}

		});

		jQuery(document).bind('DOMNodeInserted', function(e){

			if( typeof e.target.id != 'undefined' || typeof e.target.class != 'undefined'){

				if(e.target.id == 'TB_overlay'){
					jQuery('#TB_overlay').html('<div class="spinner"><div class="rect1"></div><div class="rect2"></div><div class="rect3"></div><div class="rect4"></div><div class="rect5"></div></div>');
				}

				if( jQuery(e.target).hasClass('boxPopUp2') ){
					jQuery('.boxPopUp2-overlay').html('<div class="spinner"><div class="rect1"></div><div class="rect2"></div><div class="rect3"></div><div class="rect4"></div><div class="rect5"></div></div>');
					jQuery('.boxPopUp2.vtexsm-popupContent').prepend('<a class="boxPopUp2-close" href="javascript:void(0)" />')
				}
			} 
		});

		jQuery(document).on('click', '.boxPopUp2-close', function(){
			jQuery('.boxPopUp2-overlay').trigger('click');
			jQuery.modal.close();
		});

	})( jQuery );

	function number_format(number, decimals, dec_point, thousands_sep) {

		number = (number + '').replace(/[^0-9+\-Ee.]/g, '');
	  	var n = !isFinite(+number) ? 0 : +number,
		prec = !isFinite(+decimals) ? 0 : Math.abs(decimals),
		sep = (typeof thousands_sep === 'undefined') ? ',' : thousands_sep,
		dec = (typeof dec_point === 'undefined') ? '.' : dec_point,
		s = '',
		toFixedFix = function(n, prec) {
		  var k = Math.pow(10, prec);
		  return '' + (Math.round(n * k) / k)
			.toFixed(prec);
		};
	  // Fix for IE parseFloat(0.55).toFixed(0) = 0;
	  s = (prec ? toFixedFix(n, prec) : '' + Math.round(n))
		.split('.');
	  if (s[0].length > 3) {
		s[0] = s[0].replace(/\B(?=(?:\d{3})+(?!\d))/g, sep);
	  }
	  if ((s[1] || '')
		.length < prec) {
		s[1] = s[1] || '';
		s[1] += new Array(prec - s[1].length + 1)
		  .join('0');
	  }
	  return s.join(dec);
	}

	function carrinhoUpdate(){
		var h = [];
		var total = 0;
		var itens = 0;

		vtexjs.checkout.getOrderForm().done(function(orderForm){
			jQuery('.mini-cart-itens').html('');

			for(var k in orderForm.items){
				var i = orderForm.items[k];

				if(typeof i.id == 'undefined'){
					continue;
				}

				total += i.price;
				itens += i.quantity;

				h.push('<div class="mini-cart-item item-'+ i.id +'">' +
					'   <span class="imagem">' +
					'       <a href="' + i.detailUrl + '" class="sku-imagem"><img src="' + i.imageUrl + '" alt="' + i.name + '"></a>' +
					'   </span>' +
					'   <span class="detalhes">' +
					'       <p class="nome-produto">' + i.name + '</p>' +
					'       <span class="qtd-valor">' +
					'           <span class="qtd">' + i.quantity + 'x</span>' +
					'           <span class="preco">' + i.formattedPrice + '</span>' +
					'       </span>' +
					'		<div class="item-remove"><a class="item-link-remove" data-id="'+ i.id +'" href="javascript:void(0);" title="remover"><i class="fa fa-remove item-remove-ico"></i></a></div>' +
					'   </span>' +
					'</div>');
			}

			jQuery('.mini-cart-itens').html( h.join('') );

			if( total == 0 ) {
				total = '0,00';
			} else {
				total = total.toString();
				total = total.substring(0, total.length - 2) + ',' + total.substring(total.length - 2)
			}

			jQuery('#mini-cart-admake-total').html( 'R$ ' + total) ;
			jQuery('#mini-cart-qty-admake, .mini-cart-qty-admake').html( itens );
			$('.mini-cart-header').text(` ${itens} itens no carrinho `);

		});
	} 